[alias]
  stat = status
  com = commit
  amend = commit --amend
  co = checkout
  pullrb = pull --rebase
  rbcon = rebase --continue
  br = branch
  hist = log --pretty=format:\"%h%x09%an%x09%ad%x09%s\"
  graph1 = log --graph --decorate --pretty=oneline --abbrev-commit
  graph2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[user]
  name = Ian Conery
  email = ianconery@gmail.com
